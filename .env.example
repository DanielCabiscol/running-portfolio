# Strava API Credentials
# Obtener de: https://www.strava.com/settings/api
STRAVA_CLIENT_ID=
STRAVA_CLIENT_SECRET=

# Strava OAuth Redirect URI
# Formato: https://tu-dominio.vercel.app/api/strava/callback
# Para desarrollo local: http://localhost:5173/api/strava/callback
STRAVA_REDIRECT_URI=

# OpenAI API Key
# Obtener de: https://platform.openai.com/api-keys
OPENAI_API_KEY=

# JWT Secret para encriptar tokens
# Generar con: openssl rand -base64 32
JWT_SECRET=
